#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  char Name[20];
  double S1;
  double S2;
  double S3;
  double S4;
  double S5;
  printf("Enter your name:");
  scanf("%s",Name);
  printf("Enter marks of 5 subjects:");
  scanf("%lf,%lf,%lf,%lf,%lf",&S1,&S2,&S3,&S4,&S5);
  printf("Average:%f",(S1+S2+S3+S4+S5)/5);
  return 0;
}
