#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
   double x1;
  double x2;
  double x3;
  printf("Enter 1st number x1 :"),
  scanf("%lf",&x1);
  printf("Enter 2nd number x2 :");
  scanf("%lf",&x2);
  printf("Enter 3rd number x3 :");
  scanf("%lf",&x3);
  if (x1> x2)
  {
    if (x1>x3)
    printf("x1 is greater");
    else
    printf("x2 is greater");
    
  }
  
  else if (x2>x3)
  printf("x2 is greater");
  else
  printf("x3 is greater");
  
  
  return 0;
}
